{
  "name": "@chunge16/vitepress-blogs-theme",
  "type": "module",
  "description": "VitePress easy to use blog theme",
  "version": "0.1.7",
  "main": "theme/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "changelogen --release --push && pnpm publish --access public --no-git-checks",
    "release:minor": "changelogen --release --minor --push && pnpm publish --access public",
    "release:major": "changelogen --release --major --push && pnpm publish --access public",
    "release:patch": "changelogen --release --patch --push && pnpm publish --access public"
  },
  "author": "chunge16 (https://github.com/chunge16)",
  "license": "MIT",
  "homepage": "https://github.com/chunge16/vitepress-blogs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chunge16/vitepress-blogs.git"
  },
  "keywords": [
    "vite",
    "vue",
    "vitepress",
    "theme",
    "blog"
  ],
  "exports": {
    ".": "./theme/index.js",
    "./config": "./config/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "config",
    "theme"
  ],
  "devDependencies": {
    "@jcamp/tailwindcss-plugin-icons": "^0.6.2",
    "changelogen": "^0.5.4",
    "vitepress": "1.0.0-rc.4"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}